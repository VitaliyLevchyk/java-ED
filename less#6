package Levyk;

import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.print("Введіть число: ");
        int user = scanner.nextInt();

        switch (user){
            case 5: System.out.println("Ваше число 5");
                break;
            case 50: System.out.println("Ваше число 50");
                break;
            default: System.out.println("Числа немає в кейсах!");
        }
    }
}
